function res = tau_i(tau_0)
    k = 3.06*10^(-3);
    res = tau_0 * (1 + k * 1680);
end

function res = I(t, A, tau)
    sum = 0;
    for i=1:5
        sum = sum +  A(i)*exp(-t/tau_i(tau(i)));
    end
    
    res =  sum;
end